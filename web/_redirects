# Flutter app routes - must be FIRST to take priority
/app /app/index.html 200
/app/ /app/index.html 200
/app/* /app/index.html 200

# React blog routes
/blog/story/* /index.html 200
/blog/subscriber/* /index.html 200
/blog/* /index.html 200

# API routes - pass through to Cloudflare Functions
/api/* /:splat 200

# Root - serve React landing page
/ /index.html 200

# Fallback - serve React (but won't match /app/* because it's handled above)
/* /index.html 200
